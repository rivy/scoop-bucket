{
	"version": "13.2.0-x64-nuwen",
	"description": "GNU Compiler Collection and binutils",
	"homepage": "https://www.gnu.org/software/gcc/",
	"license": "GPL-3.0-or-later",
	"architecture": {
		"64bit": {
			"url": "https://nuwen.net/files/mingw/components-19.0.7z",
			"hash": "a684b84397ba1afe1d9fb3d06cfbf2b08d8259c99045c1c2c259b04dcb09d73e"
		}
	},
	"extract_dir": "components-19.0",
	"pre_install": [
		"$f = 'binutils-2.41.7z' ; $d = \"$dir\\$f\"; Write-Host -NoNewLine \"extracting `\"$f`\"...\"; extract_7zip \"$d\" \"$dir\" ; Write-Host 'done'",
		"$f = 'mingw-w64+gcc.7z' ; $d = \"$dir\\$f\"; Write-Host -NoNewLine \"extracting `\"$f`\"...\"; extract_7zip \"$d\" \"$dir\" ; Write-Host 'done'",
		"Get-ChildItem \"$dir\\*.7z\" | Remove-Item -Recurse -Force"
	],
	"env_add_path": "bin",
	"env_set": { "C_INCLUDE_PATH": "$dir\\include", "CPLUS_INCLUDE_PATH": "$dir\\include" },
	"checkver": {
		"url": "https://nuwen.net/mingw.html",
		"regex": "(?sm)>GCC ([\\d.]+)</a>.*?components-(?<components>[\\d.]+).7z"
	},
	"autoupdate": {
		"architecture": {
			"64bit": { "url": "https://nuwen.net/files/mingw/components-$matchComponents.7z" }
		},
		"extract_dir": "components-$matchComponents"
	}
}
